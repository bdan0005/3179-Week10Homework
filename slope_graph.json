{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/bdan0005/3179-Week10Homework/refs/heads/main/slope-fertility-rate.csv"},
  "description": "Slope graph",
  "mark": "line",
  "width": {"step": 50},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "ordinal",
      "scale": {"padding": 0.5}
    },
    "y": {
      "field": "FertilityRate",
      "type": "quantitative"
    },
    "color": {"field": "State/Territory", "type": "nominal"}
  }
}